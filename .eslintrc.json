{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "ignorePatterns": ["setup.js", "*.spec.js"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-var": "error",
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}
